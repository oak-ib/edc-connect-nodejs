{
  "name": "edc-connect-nodejs",
  "version": "0.1.0",
  "description": "A minimal Electron application with node serialport connect edc",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "install": "electron-rebuild",
    "build-win64": "electron-packager . edc_connect --overwrite --asar --platform=win32 --arch=x64 --app-version=0.1.0 --out=dist --icon=assets/icon/okdev.ico",
    "build-win32": "electron-packager . edc_connect --overwrite --asar --platform=win32 --arch=ia32 --app-version=0.1.0 --out=dist --icon=assets/icon/okdev.ico"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:zokemoke/edc-connect-nodejs.git"
  },
  "keywords": [
    "Electron",
    "edc",
    "serialport",
    "his"
  ],
  "author": "Oak Isarapong",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^1.8.8",
    "electron-packager": "^14.0.1",
    "electron-rebuild": "^1.4.0"
  },
  "dependencies": {
    "buffer-split": "^1.0.0",
    "data-table": "^2.0.0",
    "ip": "^1.1.5",
    "serialport": "^7.1.5",
    "simple-node-logger": "^18.12.23",
    "socket.io-client": "^2.2.0"
  }
}
